							<!-- add target file content to target platform -->
							<target>
								<artifact>
									<groupId>org.eclipse.gef</groupId>
									<artifactId>org.eclipse.gef.target</artifactId>
									<version>5.0.0-SNAPSHOT</version>
									<classifier>NEON</classifier>
								</artifact>
							</target>
							<dependency-resolution>
								<optionalDependencies>require</optionalDependencies>
							</dependency-resolution>
							<environments>
								<environment>
									<os>linux</os>
									<ws>gtk</ws>
									<arch>x86</arch>
								</environment>
								<environment>
